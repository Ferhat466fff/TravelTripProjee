@model  List<TravelTripProjee.Models.Sınıflar.Blog>

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>


    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">

                <img src="@item.BlogImage" style="width:140px;height:80px"alt="" />
            </div>
            <div class="bot-desc">

                <a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a>
            </div>
            <div class="clear"></div>
        }


    </div>
    @Html.Action("Partial5", "Default")

</div>
